		
		
		<c:if test="${!empty requestScope.erreurs}">
			<div class="row">
					<div class="col-md-1"></div>
				<c:forEach
					items="${erreurs}"
					var="erreur">
				   	<div class="form-inline md-4 alert alert-danger"
						role="alert">${erreur}</div>
				</c:forEach>
					<div class="col-md-1"></div>
			</div>
		</c:if>
		
		
